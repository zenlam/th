<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="account_asset_maintenance_category_form" model="ir.ui.view">
            <field name="name">account.asset.category.custom.inherit.form</field>
            <field name="model">account.asset.category.custom</field>
            <field name="inherit_id" ref="odoo_account_asset.view_account_asset_category_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='company_id']" position="after">
                    <field name="asset_maintenance_team_id"/>
                </xpath>
            </field>
        </record>

    </data>
</odoo>
